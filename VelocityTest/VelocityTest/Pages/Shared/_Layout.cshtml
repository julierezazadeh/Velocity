<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - VelocityTest</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/scss/sidebar.css" />
    <link rel="stylesheet" href="~/scss/component.css" />
    <link rel="stylesheet" href="~/scss/settings.css" />
    <link rel="stylesheet" href="~/scss/variables.css" />
    <link rel="stylesheet" href="~/scss/tools.css" />
</head>
<body>
    <div class="header">
        <div class="header__title">System Settings</div>
        <button type="button" class="btn btn--primary">Save</button>
    </div>
    <div class="container-fluid">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
